int maiorInteiro(int[] array_inteiros){
	int i = 0;
	int maior_inteiro = 0;
	int valor_atual = 0;
	while(i < array_inteiros.length){
		valor_atual = array_inteiros[i];
		i = i + 1;
		if(valor_atual < maior_inteiro){
			continue;
		}
		else {
			maior_inteiro = valor_atual;
		}
	}
	return maior_inteiro;
}